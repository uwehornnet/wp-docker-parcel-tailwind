{
	"name": "acod",
	"version": "1.0.0",
	"author": "uhorn <uwehornnet@users.noreply.github.com>",
	"license": "MIT",
	"scripts": {
		"watch": "parcel watch .src/js/index.js .src/css/styles.css --dist-dir public --public-url /",
		"build": "parcel build .src/js/index.js .src/css/styles.css --dist-dir public --public-url / --no-hmr",
		"production-css": "NODE_ENV=production parcel build .src/js/index.js .src/css/styles.css --dist-dir public --public-url / --no-content-hash --no-source-maps"
	},
	"devDependencies": {
		"autoprefixer": "^10.4.2",
		"cssnano": "^5.0.15",
		"parcel": "^2.0.0-rc.0",
		"postcss": "^8.4.5",
		"postcss-import": "^14.0.2",
		"tailwindcss": "^3.0.15"
	},
	"dependencies": {
		"leaflet": "^1.7.1",
		"swiper": "^7.4.1"
	}
}
